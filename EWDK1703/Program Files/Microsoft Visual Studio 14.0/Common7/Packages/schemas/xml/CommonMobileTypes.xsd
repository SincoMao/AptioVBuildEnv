		<!-- <Item> -->
		<xsd:attributeGroup name="MobileListItemAttributes">
				<xsd:attribute name="Text" type="xsd:string" />
				<xsd:attribute name="Value" type="xsd:string" />
		</xsd:attributeGroup>
		<xsd:complexType name="MobileListItemType">
				<xsd:attributeGroup ref="MobileListItemAttributes" />
		</xsd:complexType>
		<xsd:complexType name="MobileSelectionListItemType">
				<xsd:attributeGroup ref="MobileListItemAttributes" />
				<xsd:attribute name="Selected" type="boolean" />
		</xsd:complexType>
		<!-- <Field> -->
		<xsd:complexType name="ObjectListFieldType">
				<xsd:attribute name="Name" type="xsd:string" />
				<xsd:attribute name="Title" type="xsd:string" />
				<xsd:attribute name="DataField" type="xsd:string" />
				<xsd:attribute name="DataFormatString" type="xsd:string" />
				<xsd:attribute name="Visible" type="boolean" />
