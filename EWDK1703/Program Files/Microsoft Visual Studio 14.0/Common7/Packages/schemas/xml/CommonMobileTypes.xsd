		<xsd:complexType name="bType" mixed="true">
				<xsd:choice>
						<xsd:element name="a" type="aType" form="unqualified" />
						<xsd:element name="br" type="brType" form="unqualified" />
						<xsd:element name="i" type="iType" form="unqualified" />
						<xsd:element name="p" type="pType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="iType" mixed="true">
				<xsd:choice>
						<xsd:element name="a" type="aType" form="unqualified" />
						<xsd:element name="b" type="bType" form="unqualified" />
						<xsd:element name="br" type="brType" form="unqualified" />
						<xsd:element name="p" type="pType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="pType" mixed="true">
				<xsd:choice>
						<xsd:element name="a" type="aType" form="unqualified" />
						<xsd:element name="b" type="bType" form="unqualified" />
						<xsd:element name="br" type="brType" form="unqualified" />
						<xsd:element name="i" type="iType" form="unqualified" />
