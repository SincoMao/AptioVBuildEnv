				<xsd:attribute name="OnItemCommand" vs:omtype="event" />
				<xsd:attribute name="OnItemSelect" vs:omtype="event" />
				<xsd:attribute name="OnShowItemCommands" vs:omtype="event" />
				<xsd:attribute name="OnItemDataBind" vs:omtype="event" />
