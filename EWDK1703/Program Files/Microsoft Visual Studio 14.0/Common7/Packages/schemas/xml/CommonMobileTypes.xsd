				<xsd:choice>
						<xsd:element name="DeviceSpecific" type="DeviceSpecificType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<!-- <mobile:ValidationSummary> -->
		<xsd:complexType name="ValidationSummaryType" mixed="true">
				<xsd:attributeGroup ref="MobileControlAttributes" />
				<xsd:attribute name="FormToValidate" type="xsd:string" vs:category="Behavior" />
				<xsd:attribute name="HeaderText" type="xsd:string" vs:category="Appearance" />
				<xsd:attribute name="BackLabel" type="xsd:string" vs:category="Appearance" />
				<xsd:choice>
						<xsd:element name="DeviceSpecific" type="DeviceSpecificType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<!-- Non-prefixed tabs -->
		<!-- <Style> -->
		<xsd:attributeGroup name="StyleElementAttributes">
