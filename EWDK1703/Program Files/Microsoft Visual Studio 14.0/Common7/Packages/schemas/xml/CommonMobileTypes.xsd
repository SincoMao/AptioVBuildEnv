				<xsd:attribute name="DataSource" type="xsd:string" vs:category="Data" />
				<xsd:attribute name="DataTextField" type="xsd:string" vs:category="Data" />
				<xsd:attribute name="DataValueField" type="xsd:string" vs:category="Data" />
				<xsd:attribute name="DataMember" type="xsd:string" vs:category="Data" />
				<xsd:attribute name="DataValueField" type="xsd:string" vs:category="Data" />
