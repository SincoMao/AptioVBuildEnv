				<xsd:element name="Choice" form="unqualified">
					<xsd:complexType vs:anyattribute="true">
						<xsd:attribute name="Filter" type="xsd:string" />
						<xsd:attribute name="Argument" type="xsd:string" />
						<xsd:attribute name="Xmlns" />
						<xsd:choice>
							<xsd:element name="ContentTemplate" type="TemplateType" form="unqualified" />
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="FormDeviceSpecificType">
			<xsd:attribute name="ID" type="xsd:string" />
			<xsd:attribute name="Runat" type="RunatType" />
			<xsd:choice>
				<xsd:element name="Choice" form="unqualified">
					<xsd:complexType vs:anyattribute="true">
						<xsd:attribute name="Filter" type="xsd:string" />
						<xsd:attribute name="Argument" type="xsd:string" />
						<xsd:attribute name="Xmlns" />
						<xsd:choice>
							<xsd:element name="HeaderTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="FooterTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="ScriptTemplate" type="TemplateType" form="unqualified" />
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="ListDeviceSpecificType">
			<xsd:attribute name="ID" type="xsd:string" />
			<xsd:attribute name="Runat" type="RunatType" />
			<xsd:choice>
				<xsd:element name="Choice" form="unqualified">
					<xsd:complexType vs:anyattribute="true">
						<xsd:attribute name="Filter" type="xsd:string" />
						<xsd:attribute name="Argument" type="xsd:string" />
						<xsd:attribute name="Xmlns" />
						<xsd:choice>
							<xsd:element name="HeaderTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="FooterTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="ItemTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="SeparatorTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="AlternatingItemTemplate" type="TemplateType" form="unqualified" />
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="ObjectListDeviceSpecificType">
			<xsd:attribute name="ID" type="xsd:string" />
			<xsd:attribute name="Runat" type="RunatType" />
			<xsd:choice>
				<xsd:element name="Choice" form="unqualified">
					<xsd:complexType vs:anyattribute="true">
						<xsd:attribute name="Filter" type="xsd:string" />
						<xsd:attribute name="Argument" type="xsd:string" />
						<xsd:attribute name="Xmlns" />
						<xsd:choice>
							<xsd:element name="HeaderTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="FooterTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="ItemTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="ItemDetailsTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="SeparatorTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="AlternatingItemTemplate" type="TemplateType" form="unqualified" />
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="StyleDeviceSpecificType">
			<xsd:attribute name="ID" type="xsd:string" />
			<xsd:attribute name="Runat" type="RunatType" />
			<xsd:choice>
				<xsd:element name="Choice" form="unqualified">
					<xsd:complexType vs:anyattribute="true">
						<xsd:attribute name="Filter" type="xsd:string" />
						<xsd:attribute name="Argument" type="xsd:string" />
						<xsd:attribute name="Xmlns" />
						<xsd:choice>
							<xsd:element name="ContentTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="HeaderTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="FooterTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="ItemTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="ItemDetailsTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="SeparatorTemplate" type="TemplateType" form="unqualified" />
							<xsd:element name="AlternatingItemTemplate" type="TemplateType" form="unqualified" />
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
