				<xsd:attribute name="Runat" type="RunatType" />
				<xsd:choice>
						<xsd:element name="Choice" form="unqualified">
								<xsd:complexType vs:anyattribute="true">
										<xsd:attribute name="Filter" type="xsd:string" />
										<xsd:attribute name="Argument" type="xsd:string" />
								</xsd:complexType>
						</xsd:element>
				</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="PanelDeviceSpecificType">
			<xsd:attribute name="ID" type="xsd:string" />
			<xsd:attribute name="Runat" type="RunatType" />
