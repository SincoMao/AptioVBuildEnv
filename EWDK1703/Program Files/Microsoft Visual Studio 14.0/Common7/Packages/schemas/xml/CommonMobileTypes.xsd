						<xsd:minInclusive value="-32768" />
						<xsd:maxInclusive value="32767" />
				</xsd:restriction>
		</xsd:simpleType>
		<xsd:simpleType name="i4">
				<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="-2147483648" />
						<xsd:maxInclusive value="2147483647" />
				</xsd:restriction>
		</xsd:simpleType>
		<xsd:simpleType name="ui4">
