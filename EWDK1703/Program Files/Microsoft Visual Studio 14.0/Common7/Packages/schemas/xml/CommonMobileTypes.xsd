				<xsd:attribute name="AdvertisementFile" type="xsd:string" vs:category="Behavior" />
				<xsd:attribute name="ImageKey" type="xsd:string" vs:category="Behavior" />
				<xsd:attribute name="NavigateUrlKey" type="xsd:string" vs:category="Behavior" />
