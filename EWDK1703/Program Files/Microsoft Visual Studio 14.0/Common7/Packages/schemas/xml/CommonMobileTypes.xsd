		</xsd:attributeGroup>
		<xsd:complexType name="StyleType">
				<xsd:attributeGroup ref="StyleElementAttributes" />
				<xsd:choice>
						<xsd:element name="DeviceSpecific" type="StyleDeviceSpecificType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<xsd:complexType name="PagerStyleType">
