				<xsd:attribute name="PreviousPageText" type="xsd:string" vs:category="Appearance" />
				<xsd:attribute name="PageLabel" type="xsd:string" vs:category="Appearance" />
				<xsd:choice>
						<xsd:element name="DeviceSpecific" type="StyleDeviceSpecificType" form="unqualified" />
				</xsd:choice>
