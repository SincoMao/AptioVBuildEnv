								<xsd:restriction base="xsd:string">
										<xsd:enumeration value="String" />
										<xsd:enumeration value="Integer" />
										<xsd:enumeration value="Double" />
										<xsd:enumeration value="Date" />
										<xsd:enumeration value="Currency" />
								</xsd:restriction>
						</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="MinimumValue" type="xsd:string" vs:category="Behavior" />
				<xsd:attribute name="MaximumValue" type="xsd:string" vs:category="Behavior" />
				<xsd:choice>
						<xsd:element name="DeviceSpecific" type="DeviceSpecificType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<!-- <mobile:RequiredFieldValidator> -->
		<xsd:complexType name="RequiredFieldValidatorType" mixed="true">
				<xsd:attributeGroup ref="BaseValidatorAttributes" />
				<xsd:attribute name="InitialValue" type="xsd:string" vs:category="Behavior" />
				<xsd:choice>
						<xsd:element name="DeviceSpecific" type="DeviceSpecificType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<!-- <mobile:CompareValidator> -->
		<xsd:complexType name="CompareValidatorType" mixed="true">
				<xsd:attributeGroup ref="BaseValidatorAttributes" />
				<xsd:attribute name="Type" vs:category="Behavior">
						<xsd:simpleType>
								<xsd:restriction base="xsd:string">
										<xsd:enumeration value="String" />
										<xsd:enumeration value="Integer" />
										<xsd:enumeration value="Double" />
										<xsd:enumeration value="Date" />
										<xsd:enumeration value="Currency" />
								</xsd:restriction>
						</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="Operator" vs:category="Behavior">
						<xsd:simpleType>
								<xsd:restriction base="xsd:string">
										<xsd:enumeration value="Equal" />
										<xsd:enumeration value="NotEqual" />
										<xsd:enumeration value="GreaterThan" />
										<xsd:enumeration value="GreaterThanEqual" />
										<xsd:enumeration value="LessThan" />
										<xsd:enumeration value="LessThanEqual" />
										<xsd:enumeration value="DataTypeCheck" />
								</xsd:restriction>
						</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="ValueToCompare" type="xsd:string" vs:category="Behavior" />
				<xsd:attribute name="ControlToCompare" type="xsd:string" vs:category="Behavior" />
				<xsd:choice>
						<xsd:element name="DeviceSpecific" type="DeviceSpecificType" form="unqualified" />
				</xsd:choice>
		</xsd:complexType>
		<!-- <mobile:RegularExpressionValidator> -->
		<xsd:complexType name="RegularExpressionValidatorType" mixed="true">
				<xsd:attributeGroup ref="BaseValidatorAttributes" />
				<xsd:attribute name="ValidationExpression" type="xsd:string" vs:category="Behavior" />
				<xsd:choice>
