#include "slic_base.h"

state 
{
} 
watch IoBuildSynchronousFsdRequest.exit.$return;


IoBuildSynchronousFsdRequest.exit[guard $return]
{
    if($return==NULL)
    {
        halt;
    }
}

IoFreeIrp.entry[guard $1]
{
    abort "IoFreeIrp should not be called for this irp.";
}

