#include "slic_base.h"

DbgBreakPoint@SDV.entry
{
        abort "$fname should only be called when the driver is DEBUG build";
}

DbgBreakPointWithStatus@SDV.entry
{
        abort "$fname should only be called when the driver is DEBUG build";
}
